{
  "name": "Dark GitHub",
  "version": "0.1",
  "description": "Switch the GitHub to dark mode.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [ "https://*.github.com/*" ],
      "css": [ "css/master.css" ]
    },
    {
      "matches": [ "https://github.com/" ],
      "css": [ "css/index.css" ]
    }
  ]
}
